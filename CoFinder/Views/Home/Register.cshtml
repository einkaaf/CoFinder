    <script src="../vendor/js/menu.js"></script>
    <!-- endbuild -->
    <!-- Vendors JS -->
    <script src="../vendor/libs/cleavejs/cleave.js"></script>
    <script src="../vendor/libs/cleavejs/cleave-phone.js"></script>
    <script src="../vendor/libs/moment/moment.js"></script>
    <script src="../vendor/libs/jdate/jdate.js"></script>
    <script src="../vendor/libs/flatpickr/flatpickr-jdate.js"></script>
    <script src="../vendor/libs/flatpickr/l10n/fa-jdate.js"></script>
    <script src="../vendor/libs/select2/select2.js"></script>
    <script src="../vendor/libs/select2/i18n/fa.js"></script>

    <!-- Main JS -->
    <script src="../js/main.js"></script>

    <!-- Page JS -->
    <script src="../js/form-layouts.js"></script>
</body>
</html> 